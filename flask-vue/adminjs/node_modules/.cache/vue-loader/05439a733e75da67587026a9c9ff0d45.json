-form>\r\n                <el-table\r\n                    :data=\"apps.list\"\r\n                    border\r\n                    class=\"as_table\"\r\n                    ref=\"multipleTable\"\r\n                    header-cell-class-name=\"table-header\"\r\n                >\r\n                    <el-table-column prop=\"id\" label=\"ID\" width=\"55\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"name\" label=\"名称\" width=\"200\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"description\" label=\"描述\" align=\"center\"></el-table-column>\r\n                    <el-table-column label=\"图片\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <el-image\r\n                                class=\"as_table-td-thumb\"\r\n                                :src=\"scope.row.res\"\r\n                                :preview-src-list=\"[scope.row.res]\"\r\n                            ></el-image>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n                <div class=\"pagination\">\r\n                    <el-pagination                        \r\n                        background\r\n                        :page-size=\"devicegs.pageSize\"\r\n                        :page-sizes=\"devicegs.pageSizes\"\r\n                        layout=\" total, prev, pager, next\"\r\n                        :total=\"devicegs.pageTotal\"\r\n                        @current-change=\"handleCurrentChange\"\r\n                    ></el-pagination>                \r\n                </div>\r\n            </div>\r\n        </div> \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { fetchData } from '../../../api/banner';\r\nexport default {\r\n    name: 'baseform',\r\n    data() {\r\n        return {\r\n            query: {\r\n                address: '',\r\n                name: '',\r\n                pageIndex: 1