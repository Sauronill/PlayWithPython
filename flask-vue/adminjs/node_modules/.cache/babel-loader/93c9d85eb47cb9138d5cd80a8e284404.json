ta } from '../../../api/banner';\r\nexport default {\r\n    name: 'baseform',\r\n    data() {\r\n        return {\r\n            query: {\r\n                address: '',\r\n                name: '',\r\n                pageIndex: 1,\r\n                pageSize: 10,\r\n                pageTotal: 1\r\n            },\r\n            form: {\r\n                name: '',\r\n                region: '',\r\n                desc: '',\r\n                options: []\r\n            },\r\n            devicegs: {\r\n                pageIndex: 1,\r\n                pageSize: 20,\r\n                pageTotal: 1,\r\n                list: []\r\n            },\r\n            apps: {\r\n                pageIndex: 1,\r\n                pageSize: 20,\r\n                pageTotal: 1,\r\n                list: []\r\n            },\r\n            addConditionVisible : false\r\n\r\n        };\r\n    },\r\n    created() {\r\n        this.getData();\r\n    },\r\n    methods: {\r\n        getData() {\r\n            fetchData(this.query).then(res => {\r\n                console.log(res);\r\n                this.devicegs.list = res.devicegs;\r\n                //this.devicegs.pageTotal = res.devicegs.size() || 20;\r\n                this.apps.list = res.apps;\r\n                //this.apps.pageTotal = res.apps.size() || 20;\r\n            });\r\n        },\r\n        onSubmit() {\r\n            this.$message.success('提交成功！');\r\n        },\r\n        handleAddCondition() {\r\n            this.addConditionVisible = true;\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table_margin {\r\n    margin-top: 15px;\r\n}\r\n\r\n.form {\r\n    margin-top:15px\r\n}\r\n</style>"],"sourceRoot":"src/components/page/device_group"}]}